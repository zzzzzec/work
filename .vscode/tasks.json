{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: cpp.exe build active file",
			"command": "D:\\mingw32\\bin\\g++.exe",
			"args": [
				"-std=c++17",
				"-g",
				"${workspaceFolder}\\main.cpp",
				"${workspaceFolder}\\json\\jsoncpp.cpp",
				"-o",
				"${workspaceFolder}\\main.exe",
				"-static"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
		}
	]
}
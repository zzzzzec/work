{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C++:build main",
			"command": "D:\\mingw32\\bin\\g++.exe",
			"args": [
				"-std=c++17",
				"-g",
				"${workspaceFolder}\\main.cpp",
				"${workspaceFolder}\\json\\jsoncpp.cpp",
				"-o",
				"${workspaceFolder}\\main.exe",
				"-static"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "cppbuild",
			"label": "C++:build eval",
			"command": "D:\\mingw32\\bin\\g++.exe",
			"args": [
				"-std=c++17",
				"-g",
				"${workspaceFolder}\\eval.cpp",
				"${workspaceFolder}\\json\\jsoncpp.cpp",
				"-o",
				"${workspaceFolder}\\eval.exe",
				"-static"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "cppbuild",
			"label": "C++:build eval O3",
			"command": "D:\\mingw32\\bin\\g++.exe",
			"args": [
				"-std=c++17",
				"-o3",
				"${workspaceFolder}\\eval.cpp",
				"${workspaceFolder}\\json\\jsoncpp.cpp",
				"-o",
				"${workspaceFolder}\\evalO3.exe",
				"-static"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
	]

}